											<CheckBox Grid.Column="2" Grid.Row="5" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_RPG">RPG</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="6" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_FourShot">Four Shot</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="6" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_EightShot">Eight Shot</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="6" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_WormShooterBlack">Worm Shooter Black</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="7" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_WormShooterRed">Worm Shooter Red</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="7" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_WormShooterGold">Worm Shooter Gold</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="7" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_VacuumPod">Vacuum Pod</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="8" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_LaserRifle">Laser Rifle</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="8" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_Splitter">Splitter</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="8" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_Refractor">Refractor</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="9" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_Knife">Knife</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="9" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_BlackSword">Black Sword</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="9" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_DarkHammer">Dark Hammer</CheckBox>
