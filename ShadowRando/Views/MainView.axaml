						</TabItem>
						<TabItem Header="Weapon">
							<Border Height="475">
								<ScrollViewer AllowAutoHide="False">
									<StackPanel>
										<CheckBox x:Name="Layout_Weapon_CheckBox_RandomWeaponsInWeaponBoxes" Click="Layout_Weapon_CheckBox_RandomWeaponsInWeaponBoxes_Click">Random Weapons in Weapon Boxes</CheckBox>
										<CheckBox x:Name="Layout_Weapon_CheckBox_RandomWeaponsInAllBoxes" Click="Layout_Weapon_CheckBox_RandomWeaponsInAllBoxes_Click" IsChecked="True">Random Weapons in All Boxes</CheckBox>
										<CheckBox x:Name="Layout_Weapon_CheckBox_RandomExposedWeapons" IsChecked="True">Random Exposed Weapons</CheckBox>
										<CheckBox x:Name="Layout_Weapon_CheckBox_RandomWeaponsFromEnvironment">Environment (ex: Tree / Speedlimit Sign) Drops Random Weapons</CheckBox>
										<CheckBox x:Name="Layout_Weapon_CheckBox_OnlySelectedWeapons">Only Selected Weapons</CheckBox>
										<Label Margin="0,10,0,0">Selected Weapons</Label>
										<Grid ColumnDefinitions="*, *, *" RowDefinitions="*, *, *, *, *, *, *, *, *, *, *, *, *, *">
											<CheckBox Grid.Column="0" Grid.Row="0" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_None">None</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="0" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_Pistol">Pistol</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="0" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_SubmachineGun">Submachine Gun</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="1" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_AssaultRifle">Assault Rifle</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="1" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_HeavyMachineGun">Heavy Machine Gun</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="1" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_GatlingGun">Gatling Gun</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_EggPistol">Egg Pistol</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_LightShot">Light Shot</CheckBox>
