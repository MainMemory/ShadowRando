<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:ShadowRando.ViewModels"
             xmlns:uc="clr-namespace:ShadowRando.Controls"
             xmlns:paz="using:Avalonia.Controls.PanAndZoom"
             mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="750"
             x:Class="ShadowRando.Views.MainView"
             x:DataType="vm:MainViewModel"
             Loaded="UserControl_Loaded"
             Unloaded="UserControl_Unloaded">
    <Design.DataContext>
        <!-- This only sets the DataContext for the previewer in an IDE,
         to set the actual DataContext for runtime, set the DataContext property in code (look at App.axaml.cs) -->
        <vm:MainViewModel />
    </Design.DataContext>

    <Grid RowDefinitions="*, Auto">
        <Label x:Name="LoadingDataIndicator" IsVisible="False" FontSize="40" HorizontalAlignment="Center"
               VerticalAlignment="Center" Foreground="DarkRed" Margin="10,20,0,0">
            Loading...
        </Label>
        <TabControl x:Name="MainTabParent" Grid.Row="0">
            <TabItem Header="Level Order">
                <StackPanel>
                    <StackPanel Margin="0,20,0,0" VerticalAlignment="Center" HorizontalAlignment="Center">
                        <Label x:Name="LevelOrder_Label_ProgramTitle" FontWeight="Bold" FontSize="16"
                               HorizontalAlignment="Center">
                            ShadowRando
                        </Label>
                        <Label FontWeight="Bold">Created by dreamsyntax &amp; MainMemory &amp; BlazinZzetti</Label>
                        <Button x:Name="LevelOrder_Button_ProjectPage" Margin="10,0,0,0"
                                Click="LevelOrder_Button_ProjectPage_Click" HorizontalAlignment="Center">
                            Project Page
                        </Button>
                    </StackPanel>
                    <Label>Seed</Label>
                    <Grid ColumnDefinitions="*, Auto">
                        <TextBox x:Name="LevelOrder_TextBox_Seed" Grid.Column="0"></TextBox>
                        <CheckBox x:Name="LevelOrder_CheckBox_Random_Seed"
                                  Click="LevelOrder_CheckBox_Random_Seed_Click" Grid.Column="1" Margin="10,0,10,0">
                            Random
                        </CheckBox>
                    </Grid>
                    <Label Margin="0,20,0,0">Mode</Label>
                    <ComboBox x:Name="LevelOrder_ComboBox_Mode" SelectionChanged="Shared_ComboBox_OnSelectionChanged" SelectedIndex="1" MinWidth="225">
                        <ComboBoxItem>Default Order</ComboBoxItem>
                        <ComboBoxItem>Shuffle All Stages w/Warps</ComboBoxItem>
                        <ComboBoxItem>Vanilla Structure</ComboBoxItem>
                        <ComboBoxItem>Vanilla Structure No Bosses</ComboBoxItem>
                        <ComboBoxItem>Branching Paths</ComboBoxItem>
                        <ComboBoxItem>Reverse Branching</ComboBoxItem>
                        <ComboBoxItem>Boss Rush</ComboBoxItem>
                        <ComboBoxItem>Safe Wild</ComboBoxItem>
                        <ComboBoxItem>Wild</ComboBoxItem>
                    </ComboBox>
                    <Label Margin="0,5,0,0">Main Path</Label>
                    <ComboBox x:Name="LevelOrder_ComboBox_MainPath" SelectedIndex="0" MinWidth="195">
                        <ComboBoxItem>Act Clear</ComboBoxItem>
                        <ComboBoxItem>Any Exits</ComboBoxItem>
                    </ComboBox>
                    <Grid ColumnDefinitions="*, Auto" RowDefinitions="Auto, Auto, Auto, Auto, Auto, Auto, Auto, Auto">
                        <Label Grid.Column="0" Grid.Row="0" Margin="0,10,0,0">Max Forwards Jump</Label>
                        <NumericUpDown x:Name="LevelOrder_NumericUpDown_MaxForwardsJump" Grid.Column="0" Grid.Row="1"
                                       Increment="1" Value="15" Maximum="22" Minimum="1">
                        </NumericUpDown>
                        <Label Grid.Column="0" Grid.Row="2" Margin="0,5,0,0">Max Backwards Jump</Label>
                        <NumericUpDown x:Name="LevelOrder_NumericUpDown_MaxBackwardsJump" Grid.Column="0" Grid.Row="3"
                                       Increment="1" Value="4" Maximum="21" Minimum="1">
                        </NumericUpDown>
                        <Label Grid.Column="0" Grid.Row="4" Margin="0,5,0,0">Backwards Jump Probability</Label>
                        <NumericUpDown x:Name="LevelOrder_NumericUpDown_BackwardsJumpProbability" Grid.Column="0"
                                       Grid.Row="5" Increment="1" Value="10" Maximum="100" Minimum="0">
                        </NumericUpDown>
                        <CheckBox x:Name="LevelOrder_CheckBox_AllowJumpsToSameLevel" Grid.Column="1" Grid.Row="5"
                                  Margin="10,0,10,0">
                            Allow Jumps to Same Level
                        </CheckBox>
                        <CheckBox x:Name="LevelOrder_CheckBox_AllowBossToBoss" Grid.Column="0" Grid.Row="6"
                                  Margin="0,5,0,0">
                            Allow Boss -> Boss Sequences
                        </CheckBox>
                        <CheckBox x:Name="LevelOrder_CheckBox_ExpertRando" Grid.Column="0" Grid.Row="7">Randomize Expert Mode</CheckBox>
                    </Grid>
                </StackPanel>
            </TabItem>
            <TabItem Header="Exclude Levels">
                <Panel>
                    <ScrollViewer HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible"
                                  AllowAutoHide="False">
                        <StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                <Button x:Name="Levels_Button_ToggleAll" Margin="5,0,0,0"
                                        Click="Levels_Button_ToggleAll_Click">
                                    Toggle All
                                </Button>
                                <Button x:Name="Levels_Button_ToggleBosses" Margin="5,0,0,0"
                                        Click="Levels_Button_ToggleBosses_Click">
                                    Toggle Bosses
                                </Button>
                                <Button x:Name="Levels_Button_ToggleLastStory" Margin="5,0,0,0"
                                        Click="Levels_Button_ToggleLastStory_Click">
                                    Toggle Last Story
                                </Button>
                            </StackPanel>
                            <Border BorderBrush="White" BorderThickness="1" Margin="0,0,20,20">
                                <Grid ShowGridLines="True" ColumnDefinitions="Auto, Auto, Auto, Auto, Auto, Auto, Auto"
                                      RowDefinitions="Auto, Auto, Auto, Auto, Auto, Auto, Auto, Auto, Auto, Auto">
                                    <StackPanel Grid.Column="0" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Westopolis.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_Westopolis">Westopolis</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="1" Grid.Row="2" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Digital Circuit.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_DigitalCircuit">Digital Circuit</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="1" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Glyphic Canyon.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_GlyphicCanyon">Glyphic Canyon</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="1" Grid.Row="6" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Lethal Highway.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_LethalHighway">Lethal Highway</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="1" Grid.Row="7" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Black Bull (LH).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlackBullLH">Black Bull (LH)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2" Grid.Row="2" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Cryptic Castle.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_CrypticCastle">Cryptic Castle</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2" Grid.Row="3" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Egg Breaker (CC).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_EggBreakerCC">Egg Breaker (CC)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Prison Island.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_PrisonIsland">Prison Island</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="2" Grid.Row="6" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Circus Park.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_CircusPark">Circus Park</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="0" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Central City.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_CentralCity">Central City</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="2" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/The Doom.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_TheDoom">The Doom</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="3" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Heavy Dog.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_HeavyDog">Heavy Dog</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Sky Troops.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_SkyTroops">Sky Troops</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="6" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Mad Matrix.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_MadMatrix">Mad Matrix</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="7" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Egg Breaker (MM).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_EggBreakerMM">Egg Breaker (MM)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="8" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Death Ruins.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_DeathRuins">Death Ruins</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="3" Grid.Row="9" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Black Bull (DR).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlackBullDR">Black Bull (DR)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="0" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/The ARK.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_TheARK">The ARK</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="1" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Blue Falcon.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlueFalcon">Blue Falcon</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="2" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Air Fleet.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_AirFleet">Air Fleet</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Iron Jungle.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_IronJungle">Iron Jungle</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="5" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Egg Breaker (IJ).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_EggBreakerIJ">Egg Breaker (IJ)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="6" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Space Gadget.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_SpaceGadget">Space Gadget</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="4" Grid.Row="8" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Lost Impact.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_LostImpact">Lost Impact</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="5" Grid.Row="0" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/GUN Fortress.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_GUNFortress">GUN Fortress</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="5" Grid.Row="2" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Black Comet.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlackComet">Black Comet</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="5" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Lava Shelter.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_LavaShelter">Lava Shelter</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="5" Grid.Row="6" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Cosmic Fall.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_CosmicFall">Cosmic Fall</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="5" Grid.Row="8" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Final Haunt.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_FinalHaunt">Final Haunt</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="0" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Sonic &amp; Diablon (GF).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_SonicDiablonGF">Sonic &amp; Diablon (GF)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="1" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Black Doom (GF).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlackDoomGF">Black Doom (GF)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="2" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Sonic &amp; Diablon (BC).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_SonicDiablonBC">Sonic &amp; Diablon (BC)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="3" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Egg Dealer (BC).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_EggDealerBC">Egg Dealer (BC)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="4" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Egg Dealer (LS).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_EggDealerLS">Egg Dealer (LS)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="6" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Egg Dealer (CF).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_EggDealerCF">Egg Dealer (CF)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="7" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Black Doom (CF).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlackDoomCF">Black Doom (CF)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="8" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Sonic &amp; Diablon (FH).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_SonicDiablonFH">Sonic &amp; Diablon (FH)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="6" Grid.Row="9" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Black Doom (FH).png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_BlackDoomFH">Black Doom (FH)</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="0" Grid.Row="9" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/The Last Way.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_TheLastWay">The Last Way</CheckBox>
                                    </StackPanel>
                                    <StackPanel Grid.Column="1" Grid.Row="9" Orientation="Vertical"
                                                HorizontalAlignment="Center">
                                        <Image Source="/Assets/Devil Doom.png" />
                                        <CheckBox Margin="5,0,5,0" x:Name="Levels_CheckBox_DevilDoom">Devil Doom</CheckBox>
                                    </StackPanel>
                                </Grid>
                            </Border>
                        </StackPanel>
                    </ScrollViewer>
                </Panel>
            </TabItem>
            <TabItem Header="Layout">
                <StackPanel>
                    <CheckBox x:Name="Layout_CheckBox_RandomizeLayouts" Click="Shared_CheckBox_UIUpdate_OnClick"
                              Margin="0,0,10,0">
                        Randomize Layouts (64MB Patch / Dolphin Only)
                    </CheckBox>
                    <CheckBox x:Name="Layout_CheckBox_MakeCCSplinesVehicleCompatible" Margin="0,0,10,0">Make CC Splines Vehicle Compatible</CheckBox>
                    <TabControl>
                        <TabItem Header="Enemy">
                            <StackPanel>
                                <Label>Mode</Label>
                                <ComboBox x:Name="Layout_Enemy_ComboBox_Mode"
                                          SelectionChanged="Shared_ComboBox_OnSelectionChanged" SelectedIndex="1">
                                    <ComboBoxItem>Original Enemies</ComboBoxItem>
                                    <ComboBoxItem>Wild</ComboBoxItem>
                                    <ComboBoxItem IsEnabled="False">1:1 Per Stage (Not Ready Yet)</ComboBoxItem>
                                    <ComboBoxItem IsEnabled="False">1:1 Global (Not Ready Yet)</ComboBoxItem>
                                </ComboBox>
                                <Grid ColumnDefinitions="Auto, Auto, Auto">
                                    <CheckBox x:Name="Layout_Enemy_CheckBox_AdjustMissionCounts" Grid.Column="0"
                                              IsChecked="True">
                                        Adjust Mission Counts
                                    </CheckBox>
                                    <Label Margin="10,5,5,0" Grid.Column="1">Reduction %</Label>
                                    <NumericUpDown x:Name="Layout_Enemy_NumericUpDown_AdjustMissionsReductionPercent"
                                                   Grid.Column="2" Increment="1" Value="10" Maximum="50" Minimum="0">
                                    </NumericUpDown>
                                </Grid>
                                <CheckBox x:Name="Layout_Enemy_CheckBox_KeepType" IsChecked="True">Keep Type (Flying -> Flying)</CheckBox>
                                <CheckBox x:Name="Layout_Enemy_CheckBox_KeepAffiliation" IsEnabled="False">Keep Affiliation (GUN -> GUN)</CheckBox>
                                <CheckBox x:Name="Layout_Enemy_CheckBox_RandomWeapons" IsEnabled="False">Random Weapons (Overriding 1:1 Modes)</CheckBox>
                                <Grid IsEnabled="False" ColumnDefinitions="Auto, Auto">
                                    <CheckBox x:Name="Layout_Enemy_CheckBox_ShieldBlockProbability" Grid.Column="0">Shield/Block Probability</CheckBox>
                                    <NumericUpDown x:Name="Layout_Enemy_NumericUpDown_ShieldBlockProbability"
                                                   Grid.Column="1" Increment="1" Value="10" Margin="10,0,10,0"
                                                   Maximum="100" Minimum="0">
                                    </NumericUpDown>
                                </Grid>
                                <CheckBox x:Name="Layout_Enemy_CheckBox_OnlySelectedEnemyTypes"
                                          Click="Shared_CheckBox_UIUpdate_OnClick">
                                    Only Selected Enemy Types
                                </CheckBox>
                                <Label Margin="0,10,0,0">Selected Enemy Types</Label>
                                <Grid ColumnDefinitions="*, *, *" RowDefinitions="*, *, *, *, *">
                                    <CheckBox Grid.Column="0" Grid.Row="0" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNSoldier">
                                        GUN Soldier
                                    </CheckBox>
                                    <CheckBox Grid.Column="1" Grid.Row="0" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNBeetle">
                                        GUN Beetle
                                    </CheckBox>
                                    <CheckBox Grid.Column="2" Grid.Row="0" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNBigfoot">
                                        GUN Bigfoot
                                    </CheckBox>
                                    <CheckBox Grid.Column="0" Grid.Row="1" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNRobot">
                                        GUN Robot
                                    </CheckBox>
                                    <CheckBox Grid.Column="1" Grid.Row="1" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_EggPierrot">
                                        Egg Pierrot
                                    </CheckBox>
                                    <CheckBox Grid.Column="2" Grid.Row="1" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_EggPawn">
                                        Egg Pawn
                                    </CheckBox>
                                    <CheckBox Grid.Column="0" Grid.Row="2" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_ShadowAndroid">
                                        Shadow Android
                                    </CheckBox>
                                    <CheckBox Grid.Column="1" Grid.Row="2" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAGiant">
                                        BA Giant
                                    </CheckBox>
                                    <CheckBox Grid.Column="2" Grid.Row="2" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BASoldier">
                                        BA Soldier
                                    </CheckBox>
                                    <CheckBox Grid.Column="0" Grid.Row="3" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAHawkVolt">
                                        BA Hawk/Volt
                                    </CheckBox>
                                    <CheckBox Grid.Column="1" Grid.Row="3" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAWing">
                                        BA Wing
                                    </CheckBox>
                                    <CheckBox Grid.Column="2" Grid.Row="3" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAWorm">
                                        BA Worm
                                    </CheckBox>
                                    <CheckBox Grid.Column="0" Grid.Row="4" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BALarva">
                                        BA Larva
                                    </CheckBox>
                                    <CheckBox Grid.Column="1" Grid.Row="4" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_ArtificialChaos">
                                        Artificial Chaos
                                    </CheckBox>
                                    <CheckBox Grid.Column="2" Grid.Row="4" Margin="0,0,5,0"
                                              x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAAssassin">
                                        BA Assassin
                                    </CheckBox>
                                </Grid>
                            </StackPanel>
                        </TabItem>
                        <TabItem Header="Weapon">
                            <Border x:Name="Layout_Weapon_Border" Height="475" MinHeight="475">
                                <ScrollViewer AllowAutoHide="False">
                                    <StackPanel>
                                        <CheckBox x:Name="Layout_Weapon_CheckBox_RandomWeaponsInWeaponBoxes"
                                                  Click="Layout_Weapon_CheckBox_RandomWeaponsInWeaponBoxes_Click">
                                            Random Weapons in Weapon Boxes
                                        </CheckBox>
                                        <CheckBox x:Name="Layout_Weapon_CheckBox_RandomWeaponsInAllBoxes"
                                                  Click="Layout_Weapon_CheckBox_RandomWeaponsInAllBoxes_Click"
                                                  IsChecked="True">
                                            Random Weapons in All Boxes
