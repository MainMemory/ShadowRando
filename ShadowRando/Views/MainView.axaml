											<CheckBox Grid.Column="1" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_LightShot">Light Shot</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_FlashShot">Flash Shot</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="3" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_RingShot">Ring Shot</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="3" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_HeavyShot">Heavy Shot</CheckBox>
											<CheckBox Grid.Column="2" Grid.Row="3" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_GrenadeLauncher">Grenade Launcher</CheckBox>
											<CheckBox Grid.Column="0" Grid.Row="4" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_GUNBazooka">GUN Bazooka</CheckBox>
											<CheckBox Grid.Column="1" Grid.Row="4" Margin="0,0,5,0" x:Name="Layout_Weapon_CheckBox_SelectedWeapon_TankCannon">Tank Cannon</CheckBox>
