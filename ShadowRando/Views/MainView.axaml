<UserControl xmlns="https://github.com/avaloniaui"
	 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	 xmlns:vm="clr-namespace:ShadowRando.ViewModels"
	 mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="750"
	 x:Class="ShadowRando.Views.MainView"
	 x:DataType="vm:MainViewModel"
	 Loaded="UserControl_Loaded"
     Unloaded="UserControl_Unloaded">
	<Design.DataContext>
		<!-- This only sets the DataContext for the previewer in an IDE,
         to set the actual DataContext for runtime, set the DataContext property in code (look at App.axaml.cs) -->
		<vm:MainViewModel />
	</Design.DataContext>

	<Grid RowDefinitions="*, Auto">
		<TabControl Grid.Row="0">
			<TabItem Header="Level Order">
				<StackPanel>
					<StackPanel Margin="0,20,0,0" VerticalAlignment="Center" HorizontalAlignment="Center">
						<Label x:Name="LevelOrder_Label_ProgramTitle" FontWeight="Bold" FontSize="16" HorizontalAlignment="Center">ShadowRando</Label>
						<Label FontWeight="Bold">Created by dreamsyntax &amp; MainMemory</Label>
						<Button x:Name="LevelOrder_Button_ProjectPage" HorizontalAlignment="Center">Project Page</Button>
					</StackPanel>
					<Label>Seed</Label>
					<Grid ColumnDefinitions="*, Auto">
						<TextBox x:Name="LevelOrder_TextBox_Seed" Grid.Column="0"></TextBox>
						<CheckBox x:Name="LevelOrder_CheckBox_Random_Seed" Checked="LevelOrder_CheckBox_Random_Seed_Checked" Grid.Column="1" Margin="10,0,10,0">Random</CheckBox>
					</Grid>
					<Label Margin="0,20,0,0">Mode</Label>
					<ComboBox x:Name="LevelOrder_ComboBox_Mode" SelectedIndex="1" MinWidth="225">
						<ComboBoxItem>None</ComboBoxItem>
						<ComboBoxItem>Shuffle All Stages w/Warps</ComboBoxItem>
						<ComboBoxItem>Vanilla Structure</ComboBoxItem>
						<ComboBoxItem>Branching Paths</ComboBoxItem>
						<ComboBoxItem>Reverse Branching</ComboBoxItem>
						<ComboBoxItem>Boss Rush</ComboBoxItem>
						<ComboBoxItem>Wild</ComboBoxItem>
					</ComboBox>
					<Label Margin="0,5,0,0">Main Path</Label>
					<ComboBox x:Name="LevelOrder_ComboBox_MainPath" SelectedIndex="0" MinWidth="195">
						<ComboBoxItem>Act Clear</ComboBoxItem>
						<ComboBoxItem>Any Exits</ComboBoxItem>
					</ComboBox>
					<Grid ColumnDefinitions="*, Auto" RowDefinitions="Auto, Auto, Auto, Auto, Auto, Auto">
						<Label Grid.Column="0" Grid.Row="0" Margin="0,10,0,0">Max Forwards Jump</Label>
						<NumericUpDown x:Name="LevelOrder_NumericUpDown_MaxForwardsJump" Grid.Column="0" Grid.Row="1" Increment="1" Value="15" Maximum="22" Minimum="1"></NumericUpDown>
						<Label Grid.Column="0" Grid.Row="2" Margin="0,5,0,0">Max Backwards Jump</Label>
						<NumericUpDown x:Name="LevelOrder_NumericUpDown_MaxBackwardsJump" Grid.Column="0" Grid.Row="3" Increment="1" Value="4" Maximum="21" Minimum="1"></NumericUpDown>
						<Label Grid.Column="0" Grid.Row="4" Margin="0,5,0,0">Backwards Jump Probability</Label>
						<NumericUpDown x:Name="LevelOrder_NumericUpDown_BackwardsJumpProbability" Grid.Column="0" Grid.Row="5" Increment="1" Value="10" Maximum="100" Minimum="0"></NumericUpDown>
						<CheckBox x:Name="LevelOrder_CheckBox_AllowJumpsToSameLevel" Checked="LevelOrder_CheckBox_AllowJumpsToSameLevel_Checked" Grid.Column="1" Grid.Row="5" Margin="10,0,10,0">Allow Jumps to Same Level</CheckBox>
					</Grid>
					<CheckBox x:Name="LevelOrder_CheckBox_IncludeLastStory" Margin="0,10,0,0">Include Last Story</CheckBox>
					<CheckBox x:Name="LevelOrder_CheckBox_IncludeBosses" IsThreeState="False">Include Bosses</CheckBox>
					<Label Margin="0,15,0,0">Program Theme</Label>
					<StackPanel Orientation="Horizontal">
						<ComboBox x:Name="LevelOrder_ComboBox_ProgramTheme" SelectedIndex="0">
							<ComboBoxItem>System</ComboBoxItem>
							<ComboBoxItem>Light</ComboBoxItem>
							<ComboBoxItem>Dark</ComboBoxItem>
						</ComboBox>
						<CheckBox x:Name="LevelOrder_CheckBox_ProgramSound" Margin="20,0,0,0">Program Sound</CheckBox>
					</StackPanel>
				</StackPanel>
			</TabItem>
			<TabItem Header="Layout">
				<StackPanel>
					<CheckBox x:Name="Layout_CheckBox_RandomizeLayouts" Margin="0,0,10,0">Randomize Layouts (64MB Patch / Dolphin Only)</CheckBox>
					<CheckBox x:Name="Layout_CheckBox_MakeCCSplinesVehicleCompatible" Margin="0,0,10,0">Make CC Splines Vehicle Compatible</CheckBox>
					<TabControl>
						<TabItem Header="Enemy">
							<StackPanel>
								<Label>Mode</Label>
								<ComboBox x:Name="Layout_Enemy_ComboBox_Mode" SelectedIndex="1">
									<ComboBoxItem>Default Enemies</ComboBoxItem>
									<ComboBoxItem>Wild</ComboBoxItem>
									<ComboBoxItem IsEnabled="False">1:1 Per Stage (Not Ready Yet)</ComboBoxItem>
									<ComboBoxItem IsEnabled="False">1:1 Global (Not Ready Yet)</ComboBoxItem>
								</ComboBox>
								<CheckBox x:Name="Layout_Enemy_CheckBox_AdjustMissionCounts" IsChecked="True">Adjust Mission Counts</CheckBox>
								<CheckBox x:Name="Layout_Enemy_CheckBox_KeepType" IsChecked="True">Keep Type (Flying -> Flying)</CheckBox>
								<CheckBox x:Name="Layout_Enemy_CheckBox_KeepAffiliation" IsEnabled="False">Keep Affiliation (GUN -> GUN)</CheckBox>
								<CheckBox x:Name="Layout_Enemy_CheckBox_RandomWeapons" IsEnabled="False">Random Weapons (Overriding 1:1 Modes)</CheckBox>
								<Grid IsEnabled="False" ColumnDefinitions="Auto, Auto">
									<CheckBox x:Name="Layout_Enemy_CheckBox_ShieldBlockProbability" Grid.Column="0">Shield/Block Probability</CheckBox>
									<NumericUpDown x:Name="Layout_Enemy_NumericUpDown_ShieldBlockProbability" Grid.Column="1" Increment="1" Value="10" Margin="10,0,10,0" Maximum="100" Minimum="0"></NumericUpDown>
								</Grid>
								<CheckBox x:Name="Layout_Enemy_CheckBox_OnlySelectedEnemyTypes" IsEnabled="False">Only Selected Enemy Types</CheckBox>
								<Label Margin="0,10,0,0">Selected Enemy Types</Label>
								<Grid IsEnabled="False" ColumnDefinitions="*, *, *" RowDefinitions="*, *, *, *, *">
									<CheckBox Grid.Column="0" Grid.Row="0" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNSoldier">GUN Soldier</CheckBox>
									<CheckBox Grid.Column="1" Grid.Row="0" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNBeetle">GUN Beetle</CheckBox>
									<CheckBox Grid.Column="2" Grid.Row="0" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNBigfoot">GUN Bigfoot</CheckBox>
									<CheckBox Grid.Column="0" Grid.Row="1" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_GUNRobot">GUN Robot</CheckBox>
									<CheckBox Grid.Column="1" Grid.Row="1" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_EggPierrot">Egg Pierrot</CheckBox>
									<CheckBox Grid.Column="2" Grid.Row="1" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_EggPawn">Egg Pawn</CheckBox>
									<CheckBox Grid.Column="0" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_ShadowAndroid">Shadow Android</CheckBox>
									<CheckBox Grid.Column="1" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAGiant">BA Giant</CheckBox>
									<CheckBox Grid.Column="2" Grid.Row="2" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BASoldier">BA Soldier</CheckBox>
									<CheckBox Grid.Column="0" Grid.Row="3" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAHawkVolt">BA Hawk/Volt</CheckBox>
									<CheckBox Grid.Column="1" Grid.Row="3" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAWing">BA Wing</CheckBox>
									<CheckBox Grid.Column="2" Grid.Row="3" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAWorm">BA Worm</CheckBox>
									<CheckBox Grid.Column="0" Grid.Row="4" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BALarva">BA Larva</CheckBox>
									<CheckBox Grid.Column="1" Grid.Row="4" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_ArtificialChaos">Artificial Chaos</CheckBox>
									<CheckBox Grid.Column="2" Grid.Row="4" Margin="0,0,5,0" x:Name="Layout_Enemy_CheckBox_SelectedEnemy_BAAssassin">BA Assassin</CheckBox>
								</Grid>
							</StackPanel>
						</TabItem>
						<TabItem Header="Weapon">
							<Border Height="475">
								<ScrollViewer AllowAutoHide="False">
									<StackPanel>
										<CheckBox x:Name="Layout_Weapon_CheckBox_RandomWeaponsInWeaponBoxes" IsEnabled="False">Random Weapons in Weapon Boxes</CheckBox>
